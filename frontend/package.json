    "json-loader": "0.5.4",
    "karma": "1.2.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-junit-reporter": "1.1.0",
    "karma-mocha-reporter": "2.1.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-remap-coverage": "0.1.2",
    "karma-sourcemap-loader": "0.3.7",
